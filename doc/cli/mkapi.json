{
  "description": "Documentation generator.",
  "type": "program",
  "name": "mkapi",
  "synopsis": [
    "[options] [files...]"
  ],
  "options": {
    "output": {
      "key": "output",
      "description": "Write output to FILE",
      "names": [
        "-o",
        "--output"
      ],
      "type": "option",
      "extra": "=[FILE]",
      "required": false,
      "multiple": false,
      "value": "stdout"
    },
    "title": {
      "key": "title",
      "description": "Title for initial heading",
      "names": [
        "-t",
        "--title"
      ],
      "type": "option",
      "extra": "=[VAL]",
      "required": false,
      "multiple": false
    },
    "level": {
      "key": "level",
      "description": "Initial heading level",
      "names": [
        "-l",
        "--level"
      ],
      "type": "option",
      "extra": "=[NUM]",
      "required": false,
      "multiple": false,
      "value": "1"
    },
    "lang": {
      "key": "lang",
      "description": "Language for fenced code blocks",
      "names": [
        "-L",
        "--lang"
      ],
      "type": "option",
      "extra": "=[LANG]",
      "required": false,
      "multiple": false,
      "value": "javascript"
    },
    "indent": {
      "key": "indent",
      "description": "Number of spaces for JSON",
      "names": [
        "-i",
        "--indent"
      ],
      "type": "option",
      "extra": "=[NUM]",
      "required": false,
      "multiple": false,
      "value": "2"
    },
    "ast": {
      "key": "ast",
      "description": "Print AST as JSON",
      "names": [
        "-a",
        "--ast"
      ],
      "type": "flag"
    },
    "private": {
      "key": "private",
      "description": "Enable or disable private symbols",
      "names": [
        "--[no]-private"
      ],
      "type": "flag"
    },
    "protected": {
      "key": "protected",
      "description": "Enable or disable protected symbols",
      "names": [
        "--[no]-protected"
      ],
      "type": "flag"
    },
    "help": {
      "key": "help",
      "description": "Display help and exit",
      "names": [
        "-h",
        "--help"
      ],
      "type": "flag"
    },
    "version": {
      "key": "version",
      "description": "Print the version and exit",
      "names": [
        "--version"
      ],
      "type": "flag"
    }
  }
}
