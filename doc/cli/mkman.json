{
  "description": "Render to troff man page.",
  "names": [
    "mkman"
  ],
  "type": "program",
  "name": "mkman",
  "synopsis": [
    "[options]"
  ],
  "options": {
    "title": {
      "key": "title",
      "description": "Set the page title",
      "names": [
        "-t",
        "--title"
      ],
      "type": "option",
      "extra": "=[VAL]",
      "required": false,
      "multiple": false,
      "value": "UNTITLED"
    },
    "section": {
      "key": "section",
      "description": "Set the section number",
      "names": [
        "-s",
        "--section"
      ],
      "type": "option",
      "extra": "=[NUM]",
      "required": false,
      "multiple": false,
      "value": "1"
    },
    "inline": {
      "key": "inline",
      "description": "Inline code rendering style",
      "names": [
        "-i",
        "--inline"
      ],
      "type": "option",
      "extra": "=[VAL]",
      "required": false,
      "multiple": false,
      "value": "strong"
    },
    "locale": {
      "key": "locale",
      "description": "Locale for automatic date generation",
      "names": [
        "-l",
        "--locale"
      ],
      "type": "option",
      "extra": "=[VAL]",
      "required": false,
      "multiple": false,
      "value": "en-gb"
    },
    "date": {
      "key": "date",
      "description": "Use specific date",
      "names": [
        "-d",
        "--date"
      ],
      "type": "option",
      "extra": "=[VAL]",
      "required": false,
      "multiple": false
    },
    "help": {
      "key": "help",
      "description": "Display help and exit",
      "names": [
        "-h",
        "--help"
      ],
      "type": "flag"
    },
    "version": {
      "key": "version",
      "description": "Print the version and exit",
      "names": [
        "--version"
      ],
      "type": "flag"
    }
  }
}
