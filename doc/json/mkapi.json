{
  "names": [
    "mkapi"
  ],
  "type": "program",
  "name": "mkapi",
  "summary": "documentation generator",
  "synopsis": [
    "[flags] [options] <files...>"
  ],
  "options": {
    "output": {
      "key": "output",
      "description": "Write output to FILE",
      "names": [
        "-o",
        "--output"
      ],
      "type": "option",
      "extra": "=[FILE]",
      "required": false,
      "multiple": false,
      "value": "stdout",
      "name": "--output"
    },
    "title": {
      "key": "title",
      "description": "Title for initial heading",
      "names": [
        "-t",
        "--title"
      ],
      "type": "option",
      "extra": "=[VAL]",
      "required": false,
      "multiple": false,
      "name": "--title"
    },
    "level": {
      "key": "level",
      "description": "Initial heading level",
      "names": [
        "-l",
        "--level"
      ],
      "type": "option",
      "extra": "=[NUM]",
      "required": false,
      "multiple": false,
      "value": "1",
      "name": "--level"
    },
    "lang": {
      "key": "lang",
      "description": "Language for fenced code blocks",
      "names": [
        "-L",
        "--lang"
      ],
      "type": "option",
      "extra": "=[LANG]",
      "required": false,
      "multiple": false,
      "value": "javascript",
      "name": "--lang"
    },
    "indent": {
      "key": "indent",
      "description": "Number of spaces for JSON",
      "names": [
        "-i",
        "--indent"
      ],
      "type": "option",
      "extra": "=[NUM]",
      "required": false,
      "multiple": false,
      "value": "2",
      "name": "--indent"
    },
    "ast": {
      "key": "ast",
      "description": "Print AST as JSON",
      "names": [
        "-a",
        "--ast"
      ],
      "type": "flag",
      "name": "--ast"
    },
    "cues": {
      "key": "cues",
      "description": "Enable or disable visual cues.",
      "names": [
        "--[no]-cues"
      ],
      "type": "flag",
      "name": "--[no]-cues"
    },
    "private": {
      "key": "private",
      "description": "Enable or disable private symbols",
      "names": [
        "--[no]-private"
      ],
      "type": "flag",
      "name": "--[no]-private"
    },
    "protected": {
      "key": "protected",
      "description": "Enable or disable protected symbols",
      "names": [
        "--[no]-protected"
      ],
      "type": "flag",
      "name": "--[no]-protected"
    },
    "help": {
      "key": "help",
      "description": "Display help and exit",
      "names": [
        "-h",
        "--help"
      ],
      "type": "flag",
      "name": "--help"
    },
    "version": {
      "key": "version",
      "description": "Print the version and exit",
      "names": [
        "--version"
      ],
      "type": "flag",
      "name": "--version"
    }
  }
}
