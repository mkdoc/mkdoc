{
  "names": [
    "mkman"
  ],
  "type": "program",
  "name": "mkman",
  "summary": "render to troff man page",
  "synopsis": [
    "[options]"
  ],
  "options": {
    "title": {
      "key": "title",
      "description": "Set the page title",
      "names": [
        "-t",
        "--title"
      ],
      "type": "option",
      "extra": "=[VAL]",
      "required": false,
      "multiple": false,
      "value": "UNTITLED",
      "name": "--title"
    },
    "section": {
      "key": "section",
      "description": "Set the section number",
      "names": [
        "-s",
        "--section"
      ],
      "type": "option",
      "extra": "=[NUM]",
      "required": false,
      "multiple": false,
      "value": "1",
      "name": "--section"
    },
    "inline": {
      "key": "inline",
      "description": "Inline code rendering style",
      "names": [
        "-i",
        "--inline"
      ],
      "type": "option",
      "extra": "=[VAL]",
      "required": false,
      "multiple": false,
      "value": "strong",
      "name": "--inline"
    },
    "locale": {
      "key": "locale",
      "description": "Locale for automatic date generation",
      "names": [
        "-l",
        "--locale"
      ],
      "type": "option",
      "extra": "=[VAL]",
      "required": false,
      "multiple": false,
      "value": "en-gb",
      "name": "--locale"
    },
    "preambleVersion": {
      "key": "preambleVersion",
      "description": "Version for document preamble",
      "names": [
        "-v",
        "--preamble-version"
      ],
      "type": "option",
      "extra": "=[VAL]",
      "required": false,
      "multiple": false,
      "value": "1.0",
      "name": "--preamble-version"
    },
    "date": {
      "key": "date",
      "description": "Use specific date",
      "names": [
        "-d",
        "--date"
      ],
      "type": "option",
      "extra": "=[VAL]",
      "required": false,
      "multiple": false,
      "name": "--date"
    },
    "help": {
      "key": "help",
      "description": "Display help and exit",
      "names": [
        "-h",
        "--help"
      ],
      "type": "flag",
      "name": "--help"
    },
    "version": {
      "key": "version",
      "description": "Print the version and exit",
      "names": [
        "--version"
      ],
      "type": "flag",
      "name": "--version"
    }
  }
}
